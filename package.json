{
  "name": "RacingPlus",
  "productName": "Racing+",
  "description": "A racing mod for The Binding of Isaac: Afterbirth+",
  "version": "0.27.0",
  "author": "Racing+",
  "license": "GPL-3.0",
  "homepage": "https://isaacracing.net",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zamiell/isaac-racing-client.git"
  },
  "bugs": {
    "url": "https://github.com/Zamiell/isaac-racing-client/issues"
  },
  "main": "./src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir --publish never",
    "dist": "electron-builder --publish never",
    "distPub": "electron-builder --ia32 --x64 --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "org.electron.IsaacRacingPlus",
    "win": {
      "target": "nsis-web"
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "asarUnpack": [
      "./src/lib/*",
      "./mod/**"
    ]
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-updater": "3.1.2",
    "hash-files": "^1.1.1",
    "klaw-sync": "^6.0.0",
    "linkifyjs": "^2.1.7",
    "mkdirp": "^0.5.1",
    "opn": "^5.4.0",
    "ps-node": "^0.1.6",
    "raven": "^2.6.4",
    "teeny-conf": "^1.1.0",
    "tracer": "^0.9.1",
    "winreg": "^1.2.4"
  },
  "devDependencies": {
    "electron": "3.0.5",
    "electron-builder": "20.28.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
